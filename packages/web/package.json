{
  "name": "@ks/web",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "env": "ts-node set-env.ts",
    "dev": "yarn env && ng serve --port 8020 --host 0.0.0.0 --aot",
    "build": "yarn env --environment=prod  && ng build --configuration production",
    "test": "ng test --browser ChromeHeadless --single-run",
    "test:watch": "ng test --browser ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundlesize": "bundlesize"
  },
  "bundlesize": [
    {
      "path": "./dist/main.*.js",
      "maxSize": "350 kB"
    },
    {
      "path": "./dist/polyfills.*.js",
      "maxSize": "60 kB"
    },
    {
      "path": "./dist/scripts.*.js",
      "maxSize": "50 kB"
    },
    {
      "path": "./dist/styles.*.css",
      "maxSize": "35 kB"
    }
  ],
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/core": "^0.7.0",
    "@angular-devkit/schematics": "^0.7.0",
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^6.4.1",
    "@angular/cli": "~6.0.3",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/schematics": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "bundlesize": "^0.17.0",
    "codelyzer": "~4.2.1",
    "core-js": "^2.5.4",
    "dotenv": "^6.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "rxjs": "^6.0.0",
    "ts-node": "^7.0.0",
    "web-animations-js": "^2.3.1",
    "yargs": "^12.0.1",
    "zone.js": "^0.8.26"
  }
}
